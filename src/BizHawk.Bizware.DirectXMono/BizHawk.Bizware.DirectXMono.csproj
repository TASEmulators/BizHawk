		<ProjectReference Include="$(ProjectDir)../BizHawk.Bizware.OpenTK3Mono/BizHawk.Bizware.OpenTK3Mono.csproj" />
		<Compile Include="$(ProjectDir)../BizHawk.Bizware.DirectX/**/*.cs" />
	</ItemGroup>
</Project>
